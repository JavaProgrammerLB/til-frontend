{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "til-frontend",
  "devCommand": "node dist/til-frontend/server/main.js",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/til-frontend/**"
    }
  }
}
